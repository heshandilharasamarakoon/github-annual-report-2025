{
  "name": "github-annual-report-2025",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "dev": "bun run --filter server dev & bun run --filter client dev",
    "dev:server": "cd server && bun run index.ts",
    "dev:client": "cd client && bun run dev",
    "build:client": "cd client && bun run build",
    "install:all": "cd server && bun install && cd ../client && bun install"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}

